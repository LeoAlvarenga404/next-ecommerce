{
  "name": "next-ecommerce",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "setup": "npm run setup:docker && npm run setup:deps && npm run setup:db",
    "setup:docker": "docker compose up -d",
    "setup:deps": "npm install",
    "setup:db": "npx prisma migrate dev --name init && npx prisma db seed",
