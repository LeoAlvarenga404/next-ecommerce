{
  "name": "next-ecommerce",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:stripe": "concurrently \"pnpm dev\" \"stripe listen --forward-to localhost:3000/api/webhooks/stripe\"",
    "dev:full": "pnpm setup && pnpm dev:stripe",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "setup": "pnpm setup:docker && pnpm setup:deps && pnpm setup:db",
    "setup:docker": "docker compose up -d",
    "setup:deps": "pnpm install",
    "setup:db": "pnpm dlx prisma migrate dev --name init && pnpm dlx prisma db seed",
    "reset": "docker compose down && pnpm setup",
    "seed": "node prisma/seed.js",
    "stripe:listen": "stripe listen --forward-to localhost:3000/api/webhooks/stripe"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@prisma/client": "^6.12.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@stripe/stripe-js": "^7.4.0",
    "@tanstack/react-query": "^5.82.0",
    "@tanstack/react-query-devtools": "^5.82.0",
    "@tanstack/react-table": "^8.21.3",
    "@uploadthing/react": "^7.3.2",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "embla-carousel-react": "^8.6.0",
    "jose": "^6.0.11",
    "lucide-react": "^0.525.0",
    "next": "15.3.5",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.60.0",
    "stripe": "^18.3.0",
    "tailwind-merge": "^3.3.1",
    "uploadthing": "^7.7.3",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.19.7",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19",
    "@types/stripe": "^8.0.417",
    "concurrently": "^9.2.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "prisma": "^6.12.0",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5.8.3"
  }
}
